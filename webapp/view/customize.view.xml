<mvc:View
    controllerName="zaml.controller.customize"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:wt="zaml.control"
    xmlns:st="sap.ui.comp.smarttable"
    xmlns:core="sap.ui.core"
    xmlns:co="sap.ui.comp"
>
    <Page
        id="page6"
        showNavButton="true"
        navButtonPress=".onNavBack"
        backgroundDesign="Solid"
        showHeader="true"
    >
        <content>
            <Panel>
                <content>
                    <HBox
                        direction="Row"
                        height="200px"
                        width="100%"
                        fitContainer="true"
                        wrap="NoWrap"
                        renderType="Bare"
                    >
                        <VBox
                            width="100%"
                            id="hbox104"
                        >
                            <VBox
                                height="50px"
                                width="100%"
                            />

                            <items>
                                <Button
                                    xmlns="sap.m"
                                    width="300px"
                                    press=".onOpenDialogIndustryCode"
                                    class="sapUiLargeMarginBegin"
                                    text="{i18n>INDUSTRY_CODE_HIERARCHY}"
                                    id="button3"
                                />
                                <Button
                                    xmlns="sap.m"
                                    width="300px"
                                    press=".onOpenDialogCustomerSubtype"
                                    class="sapUiLargeMarginBegin"
                                    id="button4"
                                    text="{i18n>CUSTOMER_TYPE}"
                                    
                                />
                                <Button
                                    xmlns="sap.m"
                                    width="300px"
                                    press=".onOpenDialogCountry"
                                    class="sapUiLargeMarginBegin"
                                    text="{i18n>COUNTRY_CODE}"
                                    id="button5"
                                />
                                <Button
                                    xmlns="sap.m"
                                    width="300px"
                                    press=".onOpenDialogCountryHirerchy"
                                    class="sapUiLargeMarginBegin"
                                    text="{i18n>INDUSTRY_CODE_HIERARCHY}"
                                    visible="false"
                                    id="button6"
                                />
                                <Button
                                    xmlns="sap.m"
                                    width="300px"
                                    press=".onOpenDialogRiskweightage"
                                    class="sapUiLargeMarginBegin"
                                    text="{i18n>RISK_FIELD_WEIGHTAGE}"
                                    id="button7"
                                />
                                <Button
                                    xmlns="sap.m"
                                    width="300px"
                                    press=".onOpenDialogRiskValue"
                                    class="sapUiLargeMarginBegin"
                                    text="{i18n>RISK_VALUE}"
                                    id="button8"
                                />
                                <Button
                                    xmlns="sap.m"
                                    width="300px"
                                    press=".onOpenDialogRiskValueRange"
                                    class="sapUiLargeMarginBegin"
                                    text="{i18n>RISK_VALUE_RANGE}"
                                    id="button9"
                                />
                            </items>
                        </VBox>
                        <Image
                            id="myImageId1"
                            width="3000px"
                            height="200px"
                            alt="AML Banner"
                        />
                    </HBox>
                </content>
            </Panel>
        </content>
    </Page>
</mvc:View>
